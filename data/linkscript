ENTRY(_start);
_start = 0x0C0DE000;

SECTIONS
{
    /* Start loading code from the third page (arbitrary choice) */
    . = 0x0C0DE000;
    .text ALIGN(4K) :
    {
        *(.text*)
    }

    /* Start loading data from this addres (arbitrary choice) */
    . = 0x0DA7A000;
    .data ALIGN(4K) :
    {
        *(.rodata*)
        *(.data*)
        *(COMMON)
        *(.bss*)
    }
}

